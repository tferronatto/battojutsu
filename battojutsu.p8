function states.result:init()
	self.next_state="gameover"
end

function states.result:update()
 if flash>0 then
	 flash-=1
 end

 if wait_time>0 then
	 wait_time-=1
 elseif show_result==false then
	 show_result=true
 end

 if show_result then
  local finish=check_result(p1,p2)
  if finish then
 	 sfx(3)
   if p1.life>0 then
    music(0)
   else
    music(1)
   end
   update_state()
  else
   state="play"
   init_combat()
  end
 end
 
 if can_attack then
 	ui_text=msg.reaction
	else
		ui_text=msg.fault
	end
end

function states.result:draw()
 if flash>0 then
	 cls(7)
	 spr(5,60,76)
 end
end
