		print(txt,88,106,7)
		print(flr(p1.t*16.67).."ms",93,118,7)
	else
		print(txt,88,112,7)
	end 
end

function draw_lifebars()
  print("player",1,108,7)
  print("enemy",1,118,7)
  for i=1,p1.life do
    spr(20,18+8*i,108)
  end
  for i=1,p2.life do
    spr(21,18+8*i,118)
  end
end

function draw_msgbox(x,y,w,h)
  spr(7,x,y)
  spr(8,x+w,y)
  spr(23,x,y+h)
  spr(24,x+w,y+h)
end
