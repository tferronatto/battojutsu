-- background and clouds --
function draw_bg()
  --sky
  rectfill(0,0,127,39,8)
  rectfill(0,40,127,127,14)
--  rectfill(0,70,127,79,9)
  circfill(64,190,128,9)
  line(0,37,127,37,14)
  line(0,33,127,33,14)
  line(0,31,127,31,14)
  line(0,68,127,68,14)
  line(0,66,127,66,14)
	
  --ground--
  rectfill(0,80,127,100,4)
  fillp(0b1101101010110101.1)
  rectfill(0,79,127,100,2)
  fillp()
  line(24,82,100,82,4)
  line(22,83,106,83,4)
  line(20,84,108,84,4)
  line(22,85,106,85,4)
  line(24,86,104,86,4)

  --ui area
  rectfill(0,101,127,127,0x20)
end

function init_clouds(amt)
  local c={}
  local cl=nil
  for i=1,amt do
    cl=make_cloud(flr(rnd(100)),flr(rnd(20)+20))
    add(c,cl)
  end
  return c
end

function update_clouds(c)
  for cl in all(c) do
    cl.x+=cl.dx
    if cl.x>128 then
      cl.x=-cl.w*8
    end
  end
end

function make_cloud(x,y)
  local c={}
  c.x=x
  c.y=y
  c.dx=0.05
  c.w=5
  c.h=2
  return c
end

function draw_clouds(c)
  for cl in all(c) do
    spr(10,cl.x,cl.y,cl.w,cl.h)
  end
end
